{
	"name": "RobloxAPI",
	"version": "0.0.1",
	"author": [
		"abaddriverlol"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:RobloxAPI",
	"descriptionmsg": "robloxapi-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.41.0"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\RobloxAPI\\": "src/"
	},
	"config": {
		"RobloxAPIEnabledDatasources": {
			"value": [
				"gameData",
				"groupRoles"
			],
			"merge_strategy": "provide_default",
			"description": "The datasources that should be enabled and available."
		},
		"RobloxAPIEnabledParserFunctions": {
			"value": [
				"roblox_grouprank",
				"roblox_activeplayers",
				"roblox_visits"
			],
			"merge_strategy": "provide_default",
			"description": "The parser functions that should be enabled. Data parser functions cannot be enabled or disabled here, instead they are enabled if their corresponding data source is enabled."
		},
		"RobloxAPICachingExpiries": {
			"value": {
				"gameData": 60,
				"groupRoles": 60
			},
			"description": "The amount of seconds after the cache for a data source expires"
		}
	},
	"ConfigRegistry": {
		"RobloxAPI": "GlobalVarConfig::newInstance"
	},
	"ExtensionMessagesFiles": {
		"RobloxAPIMagic": "RobloxAPI.i18n.magic.php"
	},
	"HookHandlers": {
		"RobloxAPIHooks": {
			"class": "MediaWiki\\Extension\\RobloxAPI\\Hooks"
		}
	},
	"Hooks": {
		"ParserFirstCallInit": "RobloxAPIHooks"
	},
	"MessagesDirs": {
		"RobloxAPI": [
			"i18n"
		]
	},
	"manifest_version": 2
}
